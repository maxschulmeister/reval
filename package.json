{
  "name": "reval",
  "version": "0.1.0",
  "description": "A benchmark framework for evaluating LLM applications",
  "author": "Max Schulmeister <mail@kmaxschulmeister.com> (https://github.com/maxschulmeister)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "reval": "turbo run reval",
    "db-create": "turbo run db-create",
    "db-ui": "turbo run db-ui",
    "ui": "cd packages/ui && bun dev"
  },
  "devDependencies": {
    "turbo": "^2.5.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": ">=18"
  }
